{
  "name": "ai-agent-sdk-js",
  "type": "module",
  "version": "0.0.2",
  "license": "Apache-2.0",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze -I",
    "release": "bumpp"
  },
  "dependencies": {
    "ethers": "^6.13.5",
    "valibot": "^1.0.0-beta.14"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@types/node": "^22.10.7",
    "bumpp": "^9.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "taze": "^18.3.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.11",
    "vitest": "^3.0.3"
  }
}
