{
  "name": "attps-sdk-js",
  "type": "module",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/APRO-com/ATTPs-sdk-js"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.17.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "up": "taze -I",
    "release": "bumpp"
  },
  "dependencies": {
    "bn.js": "^5.2.1",
    "elliptic": "^6.6.1",
    "ethers": "^6.13.5",
    "valibot": "^1.0.0-rc.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@types/elliptic": "^6.4.18",
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "msw": "^2.7.1",
    "taze": "^18.6.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  }
}
